{
  "name": "sunrise-web-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:e2e": "playwright test --reporter=list",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:e2e:prod": "playwright test tests/legal.spec.ts",
    "test:e2e:live": "cross-env NEXT_PUBLIC_E2E=1 playwright test tests/e2e.live.sunspire.spec.ts",
    "fast-check": "node scripts/fast-check.mjs",
    "fast:check": "scripts/fast-check-local.sh",
    "check:long": ".github/scripts/long-check-deploy.sh",
    "test:smoke": "playwright test tests/demo-smoke.spec.ts -g \"@demo-smoke\" --reporter=list",
    "test:sidebar": "playwright test tests/sidebar-cta.spec.ts -g \"@sidebar\" --reporter=list",
    "test:all-browsers": "node scripts/test-all-browsers.js",
    "test:cross-browser": "playwright test tests/cross-browser-consistency.spec.ts --reporter=list",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:estimation": "vitest run tests/estimation.unit.test.ts tests/estimation.integration.test.ts && playwright test tests/estimation.e2e.spec.ts --reporter=list",
    "prune:unused": "node scripts/prune-unused.js",
    "prune:unused:delete": "node scripts/prune-unused.js --delete"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.10",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@stripe/stripe-js": "^7.9.0",
    "@types/google.maps": "^3.58.1",
    "@vercel/kv": "^3.0.0",
    "airtable": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "nanoid": "^5.1.5",
    "next": "14.0.0",
    "nodemailer": "^7.0.5",
    "posthog-js": "^1.129.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "server-only": "^0.0.1",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "tldts": "^7.0.13",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.56.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.0.1",
    "axe-core": "^4.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "fast-glob": "^3.3.3",
    "minimatch": "^10.0.3",
    "msw": "^2.11.5",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^45.0.10",
    "vitest": "^3.2.4"
  }
}
